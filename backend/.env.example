# Node.js Environment
NODE_ENV=development
PORT=3000
NEXT_PUBLIC_API_URL=http://localhost:3000

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=query_runner
DB_USER=qrapp
DB_PASSWORD=your_secure_password

# Session Configuration
SESSION_SECRET=your_session_secret_key_generate_with_crypto

# JWT Configuration
JWT_SECRET=your_jwt_secret_key_generate_with_crypto
JWT_EXPIRY=7d

# HashiCorp Vault Configuration
VAULT_ADDR=http://localhost:8200
VAULT_TOKEN=your_vault_token
VAULT_MOUNT_PATH=secret
VAULT_NAMESPACE=

# Spring Boot Microservice Configuration
JAVA_SERVICE_URL=http://localhost:8080
JAVA_SERVICE_JWT_SECRET=your_java_service_jwt_secret

# Logging
LOG_LEVEL=info
DEBUG_MODE=false

# Concurrency Configuration (max concurrent runs per user)
MAX_CONCURRENT_RUNS=5

# Initial Admin User
INITIAL_ADMIN_USER=qradmin
INITIAL_ADMIN_PASSWORD_LOG_FILE=/var/log/query_runner/qradmin_password.log
